      <MarketValue>2000</MarketValue>
      <Flammability>0.1</Flammability>
	  <DeteriorationRate>0.1</DeteriorationRate>
	  <Beauty>5</Beauty>
      <WorkToMake>45000</WorkToMake>
      <Mass>2</Mass>
    </statBases>
    <thingSetMakerTags>
      <li>RewardSpecial</li>
    </thingSetMakerTags>
    <techHediffsTags>
      <li>Advanced</li>
	</techHediffsTags>
