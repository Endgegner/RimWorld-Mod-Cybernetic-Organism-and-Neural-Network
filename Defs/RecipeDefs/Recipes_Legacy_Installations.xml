<!-- ############## Lionheart ############## -->
 <RecipeDef ParentName="SurgeryFlesh">
    <defName>ANN_InstallLionheart</defName>
    <label>install lionheart</label>
    <description>Install a lionheart.</description>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <jobString>Installing heart.</jobString>
    <workAmount>4000</workAmount>
    <deathOnFailedSurgeryChance>0.15</deathOnFailedSurgeryChance>
	<surgerySuccessChanceFactor>0.90</surgerySuccessChanceFactor>
	<recipeUsers>
		<li>Human</li>
	</recipeUsers>
    <skillRequirements>
      <Medicine>14</Medicine>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ANN_Lionheart</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>ANN_Lionheart</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Heart</li>
    </appliedOnFixedBodyParts>
    <addsHediff>Trunken_hediff_Lionheart</addsHediff>
  </RecipeDef>

